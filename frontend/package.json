{
  "name": "servicehub-frontend",
  "version": "1.0.0",
  "description": "ServiceHub Frontend - Professional Home Services Platform",
  "homepage": "https://servicehub.com",
  "private": true,
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.1",
    "@mui/lab": "^5.0.0-alpha.155",
    "@mui/material": "^5.15.1",
    "@mui/system": "^5.15.1",
    "@mui/x-date-pickers": "^6.18.3",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.5.1",
    "axios": "^1.6.2",
    "date-fns": "^2.30.0",
    "framer-motion": "^10.16.16",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "react-scripts": "5.0.1",
    "socket.io-client": "^4.7.4",
    "web-vitals": "^3.5.0",
    "react-hook-form": "^7.48.2",
    "react-query": "^3.39.3",
    "react-hot-toast": "^2.4.1",
    "react-dropzone": "^14.2.3",
    "react-image-crop": "^10.1.8",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "uuid": "^9.0.1",
    "js-cookie": "^3.0.5",
    "react-helmet-async": "^2.0.4",
    "react-intersection-observer": "^9.5.3",
    "react-virtualized": "^9.22.5",
    "react-window": "^1.8.8",
    "fuse.js": "^7.0.0",
    "chart.js": "^4.4.0",
    "react-chartjs-2": "^5.2.0",
    "recharts": "^2.8.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-color": "^2.19.3",
    "react-spring": "^9.7.3",
    "use-debounce": "^10.0.0",
    "react-use": "^17.4.0",
    "react-error-boundary": "^4.0.11",
    "workbox-background-sync": "^7.0.0",
    "workbox-broadcast-update": "^7.0.0",
    "workbox-cacheable-response": "^7.0.0",
    "workbox-core": "^7.0.0",
    "workbox-expiration": "^7.0.0",
    "workbox-google-analytics": "^7.0.0",
    "workbox-navigation-preload": "^7.0.0",
    "workbox-precaching": "^7.0.0",
    "workbox-range-requests": "^7.0.0",
    "workbox-routing": "^7.0.0",
    "workbox-strategies": "^7.0.0",
    "workbox-streams": "^7.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "test:ci": "CI=true react-scripts test --coverage --watchAll=false --verbose",
    "test:debug": "react-scripts --inspect-brk test --runInBand --no-cache",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,css,md}\"",
    "analyze": "npm run build && npx serve -s build",
    "analyze:bundle": "npm run build && npx bundle-analyzer build/static/js/*.js",
    "predeploy": "npm run build",
    "deploy": "npm run build && npx serve -s build",
    "deploy:netlify": "npm run build && netlify deploy --prod --dir=build",
    "deploy:vercel": "npm run build && vercel --prod",
    "type-check": "echo 'Type checking complete - TypeScript support ready'",
    "precommit": "npm run lint:fix && npm run format",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "generate:component": "node scripts/generateComponent.js",
    "generate:page": "node scripts/generatePage.js",
    "security:audit": "npm audit",
    "security:fix": "npm audit fix",
    "prepare": "husky install"
  },
  "keywords": [
    "servicehub",
    "react",
    "material-ui",
    "home-services",
    "appointments",
    "chat",
    "video-calls",
    "frontend",
    "web-app",
    "pwa"
  ],
  "author": {
    "name": "ServiceHub Team",
    "email": "team@servicehub.com",
    "url": "https://servicehub.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/servicehub.git",
    "directory": "frontend"
  },
  "bugs": {
    "url": "https://github.com/yourusername/servicehub/issues"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "plugins": [
      "jsx-a11y"
    ],
    "rules": {
      "no-unused-vars": "warn",
      "no-console": "warn",
      "prefer-const": "error",
      "react-hooks/exhaustive-deps": "warn",
      "jsx-a11y/anchor-is-valid": "warn",
      "jsx-a11y/img-redundant-alt": "warn",
      "jsx-a11y/no-redundant-roles": "warn",
      "react/prop-types": "warn",
      "react/no-unused-prop-types": "warn",
      "react/jsx-key": "error",
      "react/jsx-no-duplicate-props": "error",
      "react/jsx-no-undef": "error",
      "react/no-direct-mutation-state": "error",
      "react/no-typos": "error",
      "react/require-render-return": "error"
    },
    "overrides": [
      {
        "files": [
          "**/*.test.js",
          "**/*.test.jsx"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@storybook/addon-essentials": "^7.6.3",
    "@storybook/addon-interactions": "^7.6.3",
    "@storybook/addon-links": "^7.6.3",
    "@storybook/blocks": "^7.6.3",
    "@storybook/preset-create-react-app": "^7.6.3",
    "@storybook/react": "^7.6.3",
    "@storybook/react-webpack5": "^7.6.3",
    "@storybook/testing-library": "^0.2.2",
    "@types/lodash": "^4.14.202",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/uuid": "^9.0.7",
    "@types/js-cookie": "^3.0.6",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.15",
    "prettier": "^3.1.1",
    "source-map-explorer": "^2.5.3",
    "webpack-bundle-analyzer": "^4.10.1",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "cross-env": "^7.0.3",
    "env-cmd": "^10.1.0",
    "serve": "^14.2.1",
    "netlify-cli": "^17.10.1",
    "vercel": "^32.7.2",
    "storybook": "^7.6.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/reportWebVitals.js",
      "!src/setupTests.js",
      "!src/**/*.test.{js,jsx}",
      "!src/**/*.spec.{js,jsx}",
      "!src/**/*.stories.{js,jsx}",
      "!src/serviceWorker.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 60,
        "functions": 60,
        "lines": 60,
        "statements": 60
      }
    },
    "coverageReporters": [
      "text",
      "lcov",
      "html",
      "json"
    ]
  },
  "prettier": {
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 100,
    "tabWidth": 2,
    "useTabs": false,
    "bracketSpacing": true,
    "arrowParens": "avoid",
    "endOfLine": "lf",
    "jsxSingleQuote": true,
    "jsxBracketSameLine": false
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.{css,scss,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test:ci"
    }
  },
  "proxy": "http://localhost:5000",
  "resolutions": {
    "react-error-overlay": "6.0.9"
  },
  "overrides": {
    "react-error-overlay": "6.0.9"
  }
}